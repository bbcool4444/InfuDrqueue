	mi "subsurface.mi"
	mi "subsurface_max.mi"

	link "3dsmaxshaders.dll"
	mi "3dsmaxshaders.mi"

	mi "physics_phen.mi"

	link "3dsmaxhair.dll"
	mi "3dsmaxhair.mi"
  
	link "paint.dll"
	mi "paint.mi"
	mi "paint_max.mi"
	
