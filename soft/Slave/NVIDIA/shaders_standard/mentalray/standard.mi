	mi "base.mi"
	mi "base_max.mi"

	link "contour.dll"
	mi "contour.mi"
	mi "contour_max.mi"

	link "physics.dll"
	mi "physics.mi"
	mi "physics_max.mi"

	link "lume.dll"
	mi "lume.mi"
	mi "lume_max.mi"

