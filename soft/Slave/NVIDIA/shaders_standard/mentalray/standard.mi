	mi "V2\adsk_Map_SimpleGradient.mi"
	mi "V2\adsk_Map_SimpleMarble.mi"
	mi "V2\adsk_Map_SimpleNoise.mi"
	mi "V2\adsk_Map_SimpleSpeckle.mi"
	mi "V2\adsk_Map_SimpleTile.mi"
	mi "V2\adsk_Map_SimpleWave.mi"
	mi "V2\adsk_Map_SimpleWood.mi"
