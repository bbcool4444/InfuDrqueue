<ADLMCUSTOMENV VERSION="1.0.0.0">
    <PLATFORM OS="Windows">
        <KEY ID="ADLM_COMMON_BIN_LOCATION">
            <!--Path to the AdLM shared executables-->
            <STRING>$MI_ROOT</STRING>
        </KEY>
        <KEY ID="ADLM_COMMON_LIB_LOCATION">
            <!--Path to the AdLM shared libraries-->
            <STRING>$MI_ROOT</STRING>
